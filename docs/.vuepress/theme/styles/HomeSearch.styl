    height: 28px;
}

#pages .layui-laypage a:hover {
    background: #008AFF;
    color: #fff;
}

#pages .layui-laypage .layui-disabled:hover {
    background: #F6F9FA;
}

.toTop {
    width: 60px;
    height: 60px;
    position: fixed;
    right: 30px;
    bottom: 50px;
    cursor: pointer;
    display: none;
}

.toTop img {
    width: 100%;
}


