.spcont {
    width: 100%;
    max-width: 1280px;
    margin: 55px auto 0;
    background: #fff;
    border-radius: 5px;
}

.sp-cont {
    max-width: 960px;
}

.group {
    padding: 40px 10px 0;
}

.gitem {
    overflow: hidden;
}

.gitem>span {
    display: block;
    float: left;
    font-size: 16px;
    font-weight: 500;
    color: rgba(0, 0, 0, 0.85);
    line-height: 22px;
    width: 50px;
    padding: 8px 0;
}

.gi-cont {
    margin-left: 50px;
    overflow: hidden;
}

.gi-cont .gi-item {
    display: inline-block;
    padding: 8px 15px;
    background: #F6F9FA;
    color: #68758D;
    line-height: 24px;
    font-size: 16px;
    font-weight: 500;
    border-radius: 20px;
    margin-right: 10px;
    margin-bottom: 24px;
    cursor: pointer;
}

.gi-cont .gi-item.active,.gi-cont .gi-item:hover {
    background: #008AFF;
    color: #fff;
}


