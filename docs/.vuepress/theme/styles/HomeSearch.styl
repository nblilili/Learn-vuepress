html,
body {
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}

body {
    margin: 0;
    padding: 0;
    font-size: 15px;
    font-family: Roboto, "PingFang SC" !important;
}

li {
    list-style: none;
}

a,
a:hover,
a:focus {
    text-decoration: none;
}

.position {
    position: relative;
    left: 0px;
    top: 50%;
    transform: translateY(-50%);
}

.web {
    display: block;
}

.mobile {
    display: none;
}

.bac {
    width: 100%;
    position: relative;
    overflow: hidden;
    height: 300px;
}

.bac>img {
    width: 100%;
    position: absolute;
    left: 0;
    top: 0;
    min-width: 1200px;
    height: auto;
    min-height: 300px;
}

.bactxt {
    position: absolute;
    top: 58px;
    left: 50%;
    transform: translate(-50%);
    width: 95%;
    max-width: 600px;
}

.bactxt h1 {
    font-size: 30px;
    font-weight: 500;
    color: rgba(255, 255, 255, 1);
    line-height: 42px;
    margin-top: 0;
    margin-bottom: 23px;
    text-align: center;
}

.bacsearch {
    position: relative;
}

.bacinp {
    height: 56px;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0px 2px 12px 0px rgba(0, 0, 0, 0.12);
    border-radius: 28px;
    font-size: 16px;
    padding: 6px 55px 6px 20px;
}

.bactxt p {
    font-size: 15px;
    font-weight: 500;
    color: #eee;
    line-height: 21px;
    margin-top: 12px;
    margin-left: 24px;
}

.bactxt p>span {
    cursor: pointer;
}

.bactxt p>span:hover {
    color: #fff;
}

.bsearchBtn {
    display: block;
    position: absolute;
    top: 8px;
    right: 11px;
    width: 40px;
    height: 40px;
    background: url("https://developer.juphoon.com/style/images/home/developer_search@2x.png") no-repeat;
    background-size: contain;
}

.top_search{
    position: fixed;
    top: 60px;
    left: 0;
    width: 100%;
    padding: 14px 5px;
    border-bottom: 1px solid #eee;
    z-index: 100;
    background: #F6F9FA;
}
.search_div{
    max-width: 1000px;
    margin: 0 auto;
    position: relative;
}
.top_search .bacinp{
    height: 50px;
    box-shadow: none;
}
.top_search .bsearchBtn{
    top: 6px;
    width: 38px;
    height: 38px;
    right: 7px;
}
.searchTxt{
    font-size:17px;
    color:#68758D;
    margin:10px 10px 10px 0;
    max-width: 900px;
    float: left;
}

.part {
    padding-top: 80px;
}

.part h2 {
    margin-top: 0;
    margin-bottom: 40px;
    font-weight: 300;
    font-size: 36px;
    line-height: 48px;
    text-align: center;
    color: #000;
}

.part-cont {
    width: 100%;
    max-width: 1100px;
    margin: 0 auto;
    padding: 10px;
}

.inner-cont {
    background: rgba(255, 255, 255, 1);
    box-shadow: 0px 2px 12px 0px rgba(0, 0, 0, 0.12);
    border-radius: 20px;
    padding: 30px 60px 10px 60px;
    overflow: hidden;
}

.pt-img {
    float: left;
    width: 60px;
}

.pt-img img {
    width: 100%;
}

.pt-cont {
    margin-left: 100px;
}

.pt-cont h3,
.pt-cont h3 a,
.pt-cont h3 a:hover,
.pt-cont h3 a:focus {
    font-size: 20px;
    font-weight: 500;
    color: rgba(0, 0, 0, 1);
    line-height: 28px;
    margin: 4px 0 24px;
}

.pt-mark {
    overflow: hidden;
}

.pt-item {
    float: left;
    background: #E5F8FF;
    border-radius: 20px;
    height: 40px;
    margin-right: 16px;
    cursor: pointer;
    margin-bottom: 20px;
}

.pt-item:last-child {
    margin-right: 0;
}

.pt-item a,
.pt-item a:hover,
.pt-item a:focus {
    display: block;
    padding: 8px 49px 8px 17px;
    font-size: 16px;
    font-weight: 500;
    color: rgba(104, 117, 141, 1);
    line-height: 24px;
    position: relative;
}

.pt-item i {
    position: absolute;
    display: block;
    top: 5px;
    right: 5px;
    width: 30px;
    height: 30px;
    background: url("https://developer.juphoon.com/style/images/home/developer_enter@2x.png") no-repeat;
    background-size: contain;
}

.pt-cont h3 a {
    cursor: pointer;
}

.pt-info {
    font-size: 16px;
    font-weight: 500;
    line-height: 22px;
    color: #68758D;
    overflow: hidden;
    padding-top: 6px;
}

.pt-info div {
    float: left;
    margin-right: 24px;
}

.pt-info div:last-child {
    margin-right: 0;
}

.pt-info i {
    display: inline-block;
    width: 24px;
    height: 24px;
    background: url("https://developer.juphoon.com/style/images/home/developer_cj@2x.png") no-repeat;
    background-size: contain;
    vertical-align: top;
    margin-right: 7px;
}

.inner-cont .in-item {
    padding-bottom: 24px;
}

.inner-cont .in-item:last-child {
    padding-bottom: 0;
}

.part_two .pt-cont {
    padding-bottom: 16px;
    border-bottom: 1px solid #F1F1F1;
}

.inner-cont .in-item:last-child .pt-cont {
    border: 0;
    padding-bottom: 0;
}

.pt-info .ptc-cont {
    margin-left: 88px;
    float: none;
}

.pt-info .ptc-cont>div {
    margin-bottom: 20px;
}

.pt-info .pt-icont .clear {
    clear: both;
    margin: 0;
}

.in-cont {
    text-align: center;
    padding: 20px 0;
}

.in-cont img {
    width: 60px;
    height: 60px;
}

.tit {
    font-size: 20px;
    font-weight: 500;
    color: rgba(0, 0, 0, 1);
    line-height: 28px;
    margin-top: 17px;
}

.part_four {
    text-align: center;
    padding-bottom: 120px;
    background: url("https://developer.juphoon.com/style/images/home/juphoon_voiceandvideo@2x.png") no-repeat bottom;
    background-size: contain;
}

.part_four img {
    width: 80px;
    height: 80px;
}

.part_four p {
    font-size: 32px;
    font-weight: 400;
    color: rgba(33, 33, 33, 1);
    line-height: 45px;
    margin: 20px 0 70px;
}

.part-cont .button {
    display: inline-block;
    width: 180px;
    height: 50px;
    background: rgba(0, 138, 255, 1);
    border-radius: 25px;
    font-size: 16px;
    font-weight: 500;
    color: rgba(255, 255, 255, 1);
    line-height: 50px;
    margin-right: 36px;
    cursor: pointer;
}

.contactline,
.contactline:hover,
.contactline:focus {
    font-size: 18px;
    font-weight: 400;
    color: rgba(0, 114, 255, 1);
    line-height: 25px;
    cursor: pointer;
}

.contactline i,
.si-cont i {
    display: inline-block;
    width: 12px;
    height: 11px;
    background: url("https://developer.juphoon.com/style/images/home/1to1 voice_arrow@2x.png") no-repeat;
    background-size: contain;
    margin-left: 5px;
}

