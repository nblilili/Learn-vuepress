.all {
    min-height: 1200px;
}

.mNavOut {
    height: 61px;
}

.min-nav {
    width: 100%;
    height: 61px;
}

.min-nav a {
    display: inline-block;
    color: #000;
    font-size: 15px;
    padding: 0 30px;
    line-height: 61px;
    position: relative;
}

.min-nav a:after {
    content: "";
    width: 0;
    height: 2px;
    background: #0072FF;
    position: absolute;
    bottom: 0;
    left: calc(50% + -20px);
    transition: width .5s ease-in-out;
}

.min-nav a.active:after {
    width: 41px;
}

.mn-line {
    width: 100%;
    height: 4px;
    background: #F0FBFF;
