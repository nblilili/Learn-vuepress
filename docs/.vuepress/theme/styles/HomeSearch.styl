}

.search-prompt {
    text-align: center;
}

.search_pages{
    padding: 30px 0 20px;
    width: 100%;
    max-width: 900px;
    margin: 0 auto;
    overflow: hidden;
}
.pages {
    text-align: right;
    float: right;
}

#pages .layui-laypage a,
#pages .layui-laypage span {
    background: #F6F9FA;
