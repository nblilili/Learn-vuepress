.gcont {
    width: 95%;
    max-width: 900px;
    margin: 0 auto;
    overflow: hidden;
}

.gcont .gc-item {
    padding: 20px 0;
    border-bottom: 1px solid #F5F6F8;
}

.gc-item .gtit {
    font-size: 18px;
    font-weight: 500;
    color: #000;
    line-height: 25px;
